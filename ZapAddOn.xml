<zapaddon>
    <name>Extensive OpenAPI Support</name>
    <version>1</version>
    <status>alpha</status>
    <description>Imports and scans an OpenAPI specification using predefined libraries.</description>
    <author>Sammi Fux</author>
    <url></url>
    <changes>
    </changes>
    <extensions>
        <extension>org.zaproxy.zap.extension.openapiscanner.ExtensionOpenApiScanner</extension>
    </extensions>
    <ascanrules>
    </ascanrules>
    <pscanrules>
    </pscanrules>
    <filters/>
    <files>
        <file>fuzzers/bufferoverflow.txt</file>
        <file>fuzzers/xss.txt</file>
        <file>fuzzers/sqli.txt</file>
        <file>fuzzers/xmli.txt</file>
    </files>
    <not-before-version>2.6.0</not-before-version>
    <not-from-version></not-from-version>
</zapaddon>